{
    "version": 2,
    "framework": "laravel",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.4.0"
        }
    },
    "outputDirectory": "public",
    "routes": [
        { 
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        { 
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        { 
            "src": "/build/(.*)", 
            "dest": "/public/build/$1" 
        },
        { 
            "src": "/assets/(.*)", 
            "dest": "/public/assets/$1" 
        },
        { 
            "src": "/(.*)", 
            "dest": "/api/index.php" 
        }
    ],
    "env": {
        "APP_NAME": "Laravel",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
} 